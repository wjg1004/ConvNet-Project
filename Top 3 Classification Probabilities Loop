### Top 3 Probable Leaves ###
predictions = model.predict(X_test)

pred        = predictions
top_probs   = np.empty([185,3])
max_indices = np.empty([185,3])

for i in range(1,185):
    for k in range(1,3):
        row_max          = np.max(pred[i]) #stores max value for row
        m_col            = np.argmax(pred[i]) #stores max value col index
        max_indices[i,k] = m_col
        
        top_prob[i,k] = row_max #stores max in row
        pred[i,m_col] = 0 #replaces max value with 0
