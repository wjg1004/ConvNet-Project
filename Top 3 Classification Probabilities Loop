#Status: Working

predictions = model.predict(X_test)

### Top 3 Probable Leaves ###
pred        = predictions
top_probs   = np.empty([185,3])
max_indices = np.empty([185,3])
row_max = 0
c=0
 
for k in range(3):
    for i in range(185):
        row_max          = np.max(pred[i]) #stores max value for row
        c                = np.argmax(pred[i]) #stores max value col index
        max_indices[i,k] = c
        top_probs[i,k] = row_max #stores max in row
        pred[i,c] = 0 #replaces max value with 0 
        
